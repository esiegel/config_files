[user]
	name = Eric Siegel
	email = siegel.eric@gmail.com
[core]
	editor = /usr/local/bin/vim
	excludesfile = $HOME/.gitignore
[alias]
   co = checkout
   ci = commit
   st = status
   br = branch
   hist = log --graph --date=relative --pretty=tformat:'%Cred%h%Cgreen %ad%Creset | %s%d [%an]' 
   type = cat-file -t
   dump = cat-file -p
[branch]
	autosetuprebase = always
[color]
	ui = auto
[merge]
	conflictstyle = diff3
[push]
	default = simple
[web]
	browser = open
